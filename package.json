  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@testing-library/user-event": "^13.5.0",
    "date-fns": "^3.3.1",
    "husky": "^8.0.3",
    "miragejs": "^0.1.48",
    "query-string": "^8.2.0",
    "react": "^18.2.0",
    "react-date-range": "^2.0.0-alpha.4",
    "react-dom": "^18.2.0",
    "react-image-gallery": "^1.3.0",
    "react-router-dom": "^6.21.1",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.0",
    "sass": "^1.69.7",
    "web-vitals": "^3.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npx cypress run --e2e && npx cypress run --component",
    "eject": "react-scripts eject",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "lint:check": "eslint .",
    "lint-fix": "lint-staged",
    "prepare": "husky install",
    "cypress:open": "cypress open",
    "cypress:run": "npx cypress run --e2e && npx cypress run --component"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}'",
      "eslint --fix .",
      "git add"
    ]
  },
  "devDependencies": {
    "cypress": "^13.6.3",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.1",
    "wait-on": "^7.2.0"
  }
}
