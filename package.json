  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}'",
      "eslint --fix .",
      "git add"
    ]
  },
  "devDependencies": {
    "cypress": "^13.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "lint-staged": "^15.2.2",
